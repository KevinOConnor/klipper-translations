msgstr "`MESH_RADIUS`"

msgid "`MESH_ORIGIN`"
msgstr "`MESH_ORIGIN`"

msgid "`ROUND_PROBE_COUNT`"
msgstr "`ROUND_PROBE_COUNT`"

