#~ "#dir_pin:\n"
#~ "#enable_pin:\n"
#~ "#microsteps:\n"
#~ "#rotation_distance:\n"
#~ "#endstop_pin:\n"
#~ "#position_endstop:\n"
#~ "#position_min:\n"
