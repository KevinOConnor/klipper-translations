#~ "#pin:\n"
#~ "#max_power:\n"
#~ "#shutdown_speed:\n"
#~ "#cycle_time:\n"
#~ "#hardware_pwm:\n"
#~ "#kick_start_time:\n"
#~ "#off_below:\n"
#~ "#tachometer_pin:\n"
#~ "#tachometer_ppr:\n"
#~ "#tachometer_poll_interval:\n"
#~ "#   See the \"fan\" section for a description of the above parameters.\n"
#~ "#sensor_type:\n"
#~ "#sensor_pin:\n"
#~ "#control:\n"
#~ "#pid_Kp:\n"
#~ "#pid_Ki:\n"
#~ "#pid_Kd:\n"
#~ "#pid_deriv_time:\n"
#~ "#max_delta:\n"
#~ "#min_temp:\n"
#~ "#max_temp:\n"
#~ "#   See the \"extruder\" section for a description of the above parameters.\n"
#~ "#target_temp: 40.0\n"
